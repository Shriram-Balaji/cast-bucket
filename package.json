{
  "name": "cast-bucket",
  "description": "A cross-platform podcast application",
  "version": "0.0.1",
  "author": "Shriram Balaji",
  "license": "MIT",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "web": "react-scripts start",
    "build": "react-scripts build",
    "build:stats": "react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "test:web": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "react-scripts start & electron .",
    "android-linux": "react-native bundle --platform android --dev true --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "ios-linux": "react-native bundle --entry-file='index.ios.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios' && react-native run-ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shriram-Balaji/cast-bucket.git"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/**/"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "circular-std": "^1.0.0",
    "cross-fetch": "^3.0.1",
    "node-gpodder": "^0.1.9",
    "random-hex-color": "^1.0.1",
    "react": "^16.7.0",
    "react-art": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-image": "^2.0.0",
    "react-native": "^0.58.5",
    "react-native-fast-image": "^5.1.2",
    "react-native-global-font": "^1.0.2",
    "react-native-rss-parser": "^1.3.0",
    "react-native-svg": "^9.0.2",
    "react-native-typography": "^1.3.0",
    "react-native-vector-icons": "^6.2.0",
    "react-native-web": "^0.10.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-router-native": "^4.3.0",
    "react-test-renderer": "^16.7.0",
    "recyclerlistview": "^1.3.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "title-case": "^2.1.1",
    "truncate": "^2.0.1",
    "typeface-space-mono": "0.0.71",
    "webpack-bundle-analyzer": "^3.0.4"
  },
  "devDependencies": {
    "babel-preset-react-native": "^5",
    "electron": "^4.0.2",
    "react-scripts": "^2.1.5",
    "react-test-renderer": "^16.4.1",
    "scheduler": "^0.12.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "main": "index.electron.js",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
